
@{
    ViewBag.Title = "Workout";
}

<div class="nav">

</div>
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="#">Home</a></li>
        <li class="breadcrumb-item active" aria-current="page">Dashboard</li>
    </ol>
</nav>
<main role="main">
    <div class="container">
        <div class="row">
            <div class="col-md-4 sidebar">
                <ul class="list-group">
                    <li class="list-group-item"><a href="#">Create Plan</a></li>
                    <li class="list-group-item"><a href="/Home/Workout">Add Workout</a></li>
                </ul>
            </div>
            @if (Session["Username"] != null)
            {
                if (@ViewBag.NoWorkouts != null)
                {
                    <div class="col-md-8 content" style="text-align:center;">@ViewBag.NoWorkouts</div>
                }
                else
                {
                    <div class="col-md-8 content">

                    </div>
                }
            }
        </div>
    </div>
</main>